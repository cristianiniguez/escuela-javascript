{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint",
    "migration": "ts-node ./src/db/migrator.ts",
    "migration:generate": "npm run migration -- create --name",
    "migration:run": "npm run migration -- up",
    "migration:revert": "npm run migrator -- down",
    "migration:clean": "npm run migrator -- down --to 0"
  },
  "dependencies": {
    "@faker-js/faker": "^7.4.0",
    "@hapi/boom": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.23.2",
    "umzug": "^3.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/node": "^18.7.6",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
